{
    "name": "arpesh/redis-manager",
    "description": "Redis manager for laravel",
    "type": "library",
    "keywords": ["laravel", "redis", "manager"],
    "homepage": "https://github.com/arpesh/redis-manager",
    "license": "MIT",
    "authors": [
        {
            "name": "z-song",
            "email": "zosong@126.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/contracts": "^8.37|^9.0",
        "illuminate/support": "^8.37|^9.0",
        "predis/predis": "^1.1.9|^2.0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.29 || ^9.5.23",
        "laravel/laravel": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Arpesh\\RedisManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arpesh\\RedisManager\\RedisManagerServiceProvider"
            ]

        }
    }
}
